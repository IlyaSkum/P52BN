'use strict';
let anton, ivan, mariya;
anton = (89+120+103)/3;
ivan = (116+94+123)3;
mariya = (97+134+105)/3;
alert (`среднее количество очков Антон : ${anton}`);
alert (`среднее количество очков Иван : ${ivan}`);
alert (`среднее количество очков Мария : ${mariya}`);
if ((anton > ivan) &&  (anton >mariya)) { 
  alert (`Антон набрал наибольшее количество очков: ${anton}`);
  }
  else if (mariya > ivan ) {
  alert (`Мария набрала наибольшее количество очков: ${mariya}`);
  }
  else {alert (`Иван набрал наибольшее количество очков: ${ivan}`); 
  }




